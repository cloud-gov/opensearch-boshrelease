processes:
  - name: upload_opensearch_config
    hooks:
      pre_start: /var/vcap/jobs/upload_opensearch_config/bin/pre-start
    executable: /var/vcap/jobs/upload_opensearch_config/bin/job.process
    ephemeral_disk: true
    additional_volumes:
    - path: /var/vcap/jobs/opensearch_templates
    - path: /var/vcap/jobs/opensearch/config
      writable: true
