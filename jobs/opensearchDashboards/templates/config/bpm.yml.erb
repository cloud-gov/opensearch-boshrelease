processes:
  - name: opensearchDashboards
    hooks:
      pre_start: /var/vcap/jobs/opensearchDashboards/bin/pre-start
    executable: /var/vcap/jobs/opensearchDashboards/bin/opensearchDashboards.sh
    ephemeral_disk: true
    additional_volumes:
    <% p("opensearchDashboards.source_files").each do |sf| %>
    - path:  <%= sf %>
    <% end %>
    - path: /var/vcap/data/packages/opensearchDashboards
      writable: true
      allow_executions: true
