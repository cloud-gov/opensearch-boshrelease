processes:
  - name: opensearch-dashboard
    hooks:
      pre_start: /var/vcap/jobs/opensearch-dashboard/bin/pre-start
    executable: /var/vcap/jobs/opensearch-dashboard/bin/opensearch-dashboard.sh
    ephemeral_disk: true
    additional_volumes:
    <% p("opensearch-dashboard.source_files").each do |sf| %>
    - path:  <%= sf %>
    <% end %>
    - path: /var/vcap/data/packages/opensearch-dashboard
      writable: true
      allow_executions: true
